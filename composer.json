{
  "name": "borschphp/skeleton",
  "description": "A Borsch Framework skeleton application to kick start development.",
  "homepage": "http://github.com/borschphp/borsck-skeleton",
  "license": "MIT",
  "keywords": [
    "borsch",
    "skeleton",
    "middleware",
    "psr",
    "psr-7",
    "psr-15"
  ],
  "authors": [
    {
      "name": "Alexandre DEBUSSCHERE",
      "email": "zizilex@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "borschphp/container": "0.2",
    "borschphp/application": "0.3",
    "borschphp/requesthandler": "0.1",
    "borschphp/router": "0.1",
    "borschphp/smarty": "0.1",
    "laminas/laminas-diactoros": "^2.3",
    "laminas/laminas-db": "^2.11",
    "monolog/monolog": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    },
    "files": [
      "config/alias.php"
    ]
  }
}
